  <required>
   <php>
    <min>5.3.3</min>
   </php>
   <pearinstaller>
    <min>1.9.4</min>
   </pearinstaller>
   <package>
    <name>File_Iterator</name>
    <channel>pear.phpunit.de</channel>
    <min>1.3.0</min>
   </package>
   <package>
    <name>PHP_TokenStream</name>
    <channel>pear.phpunit.de</channel>
    <min>1.1.3</min>
   </package>
   <package>
    <name>Text_Template</name>
    <channel>pear.phpunit.de</channel>
    <min>1.1.1</min>
   </package>
  </required>
  <optional>
   <extension>
    <name>dom</name>
   </extension>
   <extension>
    <name>xdebug</name>
    <min>2.0.5</min>
   </extension>
  </optional>
 </dependencies>
 <phprelease />
</package>
